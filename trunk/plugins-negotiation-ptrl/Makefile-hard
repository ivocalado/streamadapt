all: compile_message compile_xmppnegotiation build_all

compile_message:
	g++ message.cpp -c 

compile_xmppnegotiation:
	g++ -I/home/heverton/workspace/streamadapt/inc -O0 -g3 -fpermissive -Wall -c -fmessage-length=0 -fPIC -MMD -MP -MF"xmppnegotiation.d" -MT"xmppnegotiation.d" -o"xmppnegotiation.o" "xmppnegotiation.cpp"

build_all:
	g++ -shared -o "libplugins-negotiation.so" -lgloox -pthread -lccgnu2

clean:
	rm -rf *.o *.so
